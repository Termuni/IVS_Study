str_ = "0123456789"
str_[0:3]  #0번 index부터 3번 index 바로 앞까지 slicing


str_[-1]   #음수 인덱스는 뒤에서부터 라는 뜻으로 읽힘


str_[-5:-1] #뒤에서부터 출력


str_[5:] #5번부터 끝까지


