result = [fruit for fruit in fruits if len(fruit) >= 5]
result


#과일 글자가 5글자 이상인 과일의 글자수 출력
fruits = ["orange", "apple", "mango", "kiwi"]
result = [len(fruit) for fruit in fruits if len(fruit) >= 5]
result





